block search
    .home_search
        .container
          .row
            .col
              .home_search_container
                .home_search_content
                  form.search_form.d-flex.flex-row.align-items-start.justfy-content-start(action='#')
                    .search_form_content.d-flex.flex-row.align-items-start.justfy-content-start.flex-wrap
                      div
                        select.search_form_select
                          option(disabled='', selected='') For rent
                          option Yes
                          option No
                      div
                        select.search_form_select
                          option(disabled='', selected='') All types
                          option Type 1
                          option Type 2
                          option Type 3
                          option Type 4
                      div
                        select.search_form_select
                          option(disabled='', selected='') City
                          option New York
                          option Paris
                          option Amsterdam
                          option Rome
                      div
                        select.search_form_select
                          option(disabled='', selected='') Bedrooms
                          option 1
                          option 2
                          option 3
                          option 4
                      div
                        select.search_form_select
                          option(disabled='', selected='') Bathrooms
                          option 1
                          option 2
                          option 3
                    button.search_form_button.ml-auto search

